@page
@model WebAppRazorPages.Pages.UsersModel
@{
    ViewData["Title"] = "Cars page";
}

<h1>Страница информации о ТС</h1>
<a asp-page="EditUser" class="btn btn-primary stretched-link">Создать</a>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Марка ТС</th>
            <th>Модель ТС</th>
            <th>Тип двигателя</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var user in Model.users)
    {
        <tr>
            <td><a href="EditUser?id_car="+@user.Id_car>@user.Id_car</a></td>
            <td>@user.BrandCar</td>
            <td>@user.Model</td>
            <td>@user.EngineCar</td>
        </tr>
    }
    </tbody>
</table>
